{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true
    },
  "exclude": [
    "node_modules",
    "lib",
    "res",
    "typings/main",
    "typings/main.d.ts",
    "typings/otherStuff.d.ts"
  ]
}
